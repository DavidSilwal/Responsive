{
  "description": "ASP.NET Core client responsiveness middleware for routing base upon request client device detection to specific view",
  "version": "1.0.0-*",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/wangkanai/Responsiveness"
    },
    "tags": [
      "aspnetcore",
      "responsiveness"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Abstractions": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "Wangkanai.AspNetCore.Responsiveness.Core": "1.0.0-*"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  }
}
